<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Hotels Automation Script " parallel="methods" thread-count="4">

    <parameter name="platform" value="mac"/>
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="53.0"/>
    <parameter name="url" value="https://www.hotels.com/"/>

    <test name="Hotels Home" parallel="none">
        <classes>
            <class name="TestHomePage"/>
          <!--  <class name="TestPackages_Flights"/>
            <class name="TestGiftCard"/>
            <class name="TestGroupMeeting"/>

            <class name="TestCreateAccount"/>
            <class name="TestSignIn"/>-->

        </classes>
    </test>
</suite>